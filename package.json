    "url": "https://github.com/sparky-raccoon/epitome/issues"
  },
  "homepage": "https://github.com/sparky-raccoon/epitome#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "discord.js": "^14.14.0",
    "dotenv": "^16.0.0",
    "node-cron": "^3.0.3",
    "rss-parser": "^3.12.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.6.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.1.2",
    "typescript": "5"
